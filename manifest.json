{
    "name" : "GuardianWebShield",
    "version" : "0.0.1",
    "description" : "Website blocker",
    "manifest_version" : 2,
    "permissions" : ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background" : {
        "scripts" : ["background.js"]
    },
    "icons" : {
        "16" : "shield.png",
        "48" : "shield.png",
        "128" : "shield.png"
    }
}